<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <merge tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="2dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="2dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_end="2dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="2dp" />

        <View
            android:id="@+id/cell_0_0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_1_0"
            app:layout_constraintEnd_toStartOf="@id/cell_0_1"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toTopOf="@id/guideline_top" />

        <View
            android:id="@+id/cell_0_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_1_1"
            app:layout_constraintEnd_toStartOf="@id/cell_0_2"
            app:layout_constraintStart_toEndOf="@id/cell_0_0"
            app:layout_constraintTop_toTopOf="@id/guideline_top" />

        <View
            android:id="@+id/cell_0_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_1_2"
            app:layout_constraintEnd_toStartOf="@id/cell_0_3"
            app:layout_constraintStart_toEndOf="@id/cell_0_1"
            app:layout_constraintTop_toTopOf="@id/guideline_top" />

        <View
            android:id="@+id/cell_0_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_1_3"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintStart_toEndOf="@id/cell_0_2"
            app:layout_constraintTop_toTopOf="@id/guideline_top" />


        <View
            android:id="@+id/cell_1_0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="1dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_2_0"
            app:layout_constraintEnd_toStartOf="@id/cell_1_1"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@id/cell_0_0" />

        <View
            android:id="@+id/cell_1_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="1dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_2_1"
            app:layout_constraintEnd_toStartOf="@id/cell_1_2"
            app:layout_constraintStart_toEndOf="@id/cell_1_0"
            app:layout_constraintTop_toBottomOf="@id/cell_0_1" />

        <View
            android:id="@+id/cell_1_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="1dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_2_2"
            app:layout_constraintEnd_toStartOf="@id/cell_1_3"
            app:layout_constraintStart_toEndOf="@id/cell_1_1"
            app:layout_constraintTop_toBottomOf="@id/cell_0_1" />

        <View
            android:id="@+id/cell_1_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginBottom="1dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_2_3"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintStart_toEndOf="@id/cell_1_2"
            app:layout_constraintTop_toBottomOf="@id/cell_0_1" />


        <View
            android:id="@+id/cell_2_0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_3_0"
            app:layout_constraintEnd_toStartOf="@id/cell_2_1"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@id/cell_1_0" />

        <View
            android:id="@+id/cell_2_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_3_1"
            app:layout_constraintEnd_toStartOf="@id/cell_2_2"
            app:layout_constraintStart_toEndOf="@id/cell_2_0"
            app:layout_constraintTop_toBottomOf="@id/cell_1_1" />

        <View
            android:id="@+id/cell_2_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="0.5dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_3_2"
            app:layout_constraintEnd_toStartOf="@id/cell_2_3"
            app:layout_constraintStart_toEndOf="@id/cell_2_1"
            app:layout_constraintTop_toBottomOf="@id/cell_1_2" />

        <View
            android:id="@+id/cell_2_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@id/cell_3_3"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintStart_toEndOf="@id/cell_2_2"
            app:layout_constraintTop_toBottomOf="@id/cell_1_3" />


        <View
            android:id="@+id/cell_3_0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            app:layout_constraintEnd_toStartOf="@id/cell_3_1"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@id/cell_2_0" />

        <View
            android:id="@+id/cell_3_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="1dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            app:layout_constraintEnd_toStartOf="@id/cell_3_2"
            app:layout_constraintStart_toEndOf="@id/cell_3_0"
            app:layout_constraintTop_toBottomOf="@id/cell_2_1" />

        <View
            android:id="@+id/cell_3_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="0.5dp"
            android:layout_marginEnd="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            app:layout_constraintEnd_toStartOf="@id/cell_3_3"
            app:layout_constraintStart_toEndOf="@id/cell_3_1"
            app:layout_constraintTop_toBottomOf="@id/cell_2_2" />

        <View
            android:id="@+id/cell_3_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0.5dp"
            android:layout_marginTop="0.5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintStart_toEndOf="@id/cell_3_2"
            app:layout_constraintTop_toBottomOf="@id/cell_2_3" />

    </merge>
</layout>
